class Demo
{
    private int a,b;
	Demo(int a,int b)
	{
	this.a=a;
	this.b=b;
	}
	
	int sum()
	{
	
	return(a+b);
	}



}

class Demo1  extends Demo
{    
    int c;
    Demo1(int x,int y,int z)
    {
	super(x,y);
	c=z;
	
	
	}   

    void sum1()
    {
	System.out.println("sum is "+(sum()+c));
	
	}	



}

class Superu
{
public static void main(String args[])
{

   Demo1 d1=new Demo1(1,3,4);
   d1.sum1();

}
}
