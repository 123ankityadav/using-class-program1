class Demo
{
    void same()
	{
	System.out.println("my name is ankit");
	}
}

class Demo1 extends Demo
{

     @Override
     void same()
	 {
	 System.out.println("my name is yadav");
	 }
	 
	 void recall()
	 {
	 super.same();
	 }
}

class Super3
{
public static void main(String args[])
{
     Demo1 d1=new Demo1();
	 d1.same();
	 d1.recall();
     

}
}
