class Concat
{
public static void main(String args[])
{
     int a=34;
	 int b=90;
	 String s="the age is "+a+""+b+" of ankit";
     System.out.println(s);
	 
	 String s1="age is"+90;
	 System.out.println(s1);
	 
	 String s2="age is"+"2"+"8";
	 System.out.println(s2);
	 
	 String s3="ankit"+"rocks"+"ajay";
	 System.out.println(s3);
}
}
